        "target": "ES5",
        "module": "commonjs",
        "declaration": true,
        "sourceMap": true,
        "rootDir": "./src",
        "outDir": "./lib",
        "pretty": true,
        "stripInternal": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "noFallthroughCasesInSwitch": false,
